{
    "name": "webcityro/larasearch",
    "description": "The easy way to do the \"R\" in CRUD.",
    "version": "1.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrei Valcu",
            "email": "andreivalcu@gmail.com"
        }
    ],
    "require": {
        "php": "8.*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.2",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Webcityro\\Larasearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webcityro\\Larasearch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webcityro\\Larasearch\\LarasearchServiceProvider"
            ]
        }
    }
}
